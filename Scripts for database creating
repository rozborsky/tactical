CREATE DATABASE tactic
  WITH ENCODING='UTF8'
       CONNECTION LIMIT=-1;

CREATE TABLE public."homePageGoodsList"
(
   id integer PRIMARY KEY DEFAULT nextval('id_increment'),
   goods text NOT NULL,
   image text NOT NULL
)

CREATE SEQUENCE public.id_increment
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 999999
  START 1
  CACHE 1;