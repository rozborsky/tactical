CREATE DATABASE tactic
  WITH ENCODING='UTF8'
       CONNECTION LIMIT=-1;


CREATE TABLE [homePageGoodsList](
    [id] INTEGER(2),
    [goods] TEXT(50) NOT NULL,
    [image] TEXT(50) NOT NULL,
    PRIMARY KEY([id]));


CREATE TABLE [buyer](
    [id] INTEGER(3),
    [name] TEXT(20) NOT NULL,
    [surname] TEXT(20) NOT NULL,
    [eMail] TEXT(50) NOT NULL,
    [address] TEXT(50) NOT NULL,
    [phone] TEXT(15) NOT NULL,
    [password] TEXT(50),
    PRIMARY KEY([id]));

SQLite

INSERT INTO homePageGoodsList(
            goods, image)
    VALUES ('badges', 'badges.jpg'),
    ('closeout', 'closeout.jpg'),
